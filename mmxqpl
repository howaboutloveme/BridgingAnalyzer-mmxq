
        if (label.equalsIgnoreCase("skyblock") || label.equalsIgnoreCase("sb")) {
            if (args.length == 0) {
                if (sender instanceof Player) {
                    Player player = (Player) sender;
                    // 列出指令列表
                    if (player.hasPermission("skyblock.admin")) {
                        getLogger().info("§rUnknown command. Type \"/help\" for help.");
                    } else {
                        player.sendMessage(message.getString("no-permission"));
                    }
                } else if (sender instanceof ConsoleCommandSender) {
                    getLogger().info("§rUnknown command. Type \"/help\" for help.");
